{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "target": "es2016",
        "module": "esnext",
        "rootDir": "./frontend/src",
        "outDir": "./frontend/public/src",
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./frontend/src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}